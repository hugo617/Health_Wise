    <script>
        class ModernUploader {
            constructor() {
                this.dropZone = document.getElementById('dropZone');
                this.fileInput = document.getElementById('fileInput');
                this.fileInfoCard = document.getElementById('fileInfoCard');
                this.uploadBtn = document.getElementById('uploadBtn');
                this.uploadProgress = document.getElementById('uploadProgress');
                this.progressBar = document.getElementById('progressBar');
                this.progressPercentage = document.getElementById('progressPercentage');
                this.uploadedSize = document.getElementById('uploadedSize');
                this.uploadSpeed = document.getElementById('uploadSpeed');
                this.remainingTime = document.getElementById('remainingTime');
                this.statusMessage = document.getElementById('statusMessage');
                
                this.selectedFile = null;
                this.selectedReportType = null;
                this.uploadStartTime = null;
                this.lastUploadedBytes = 0;
                
                this.initEventListeners();
            }

            initEventListeners() {
                // æ‹–æ‹½äº‹ä»¶
                this.dropZone.addEventListener('click', () => this.fileInput.click());
                this.dropZone.addEventListener('dragover', this.handleDragOver.bind(this));
                this.dropZone.addEventListener('dragleave', this.handleDragLeave.bind(this));
                this.dropZone.addEventListener('drop', this.handleDrop.bind(this));
                
                // æ–‡ä»¶é€‰æ‹©äº‹ä»¶
                this.fileInput.addEventListener('change', this.handleFileSelect.bind(this));
                
                // ä¸Šä¼ æŒ‰é’®äº‹ä»¶
                this.uploadBtn.addEventListener('click', this.handleUpload.bind(this));
                
                // æŠ¥å‘Šç±»å‹é€‰æ‹©äº‹ä»¶
                document.querySelectorAll('.report-type-card').forEach(card => {
                    card.addEventListener('click', this.handleReportTypeSelect.bind(this));
                });
            }

            handleDragOver(e) {
                e.preventDefault();
                this.dropZone.classList.add('drag-over');
            }

            handleDragLeave(e) {
                e.preventDefault();
                this.dropZone.classList.remove('drag-over');
            }

            handleDrop(e) {
                e.preventDefault();
                this.dropZone.classList.remove('drag-over');
                
                const files = e.dataTransfer.files;
                if (files.length > 0) {
                    this.processFile(files[0]);
                }
            }

            handleFileSelect(e) {
                const file = e.target.files[0];
                if (file) {
                    this.processFile(file);
                }
            }

            processFile(file) {
                // éªŒè¯æ–‡ä»¶ç±»å‹
                if (file.type !== 'application/pdf') {
                    this.showStatus('è¯·é€‰æ‹© PDF æ ¼å¼çš„æ–‡ä»¶', 'error');
                    return;
                }

                // éªŒè¯æ–‡ä»¶å¤§å°
                const maxSize = 500 * 1024 * 1024; // 500MB
                if (file.size > maxSize) {
                    this.showStatus('æ–‡ä»¶å¤§å°è¶…è¿‡é™åˆ¶ï¼ˆæœ€å¤§ 500MBï¼‰', 'error');
                    return;
                }

                this.selectedFile = file;
                this.displayFileInfo(file);
                this.updateUploadButton();
            }

            displayFileInfo(file) {
                document.getElementById('fileName').textContent = file.name;
                document.getElementById('fileSize').textContent = `å¤§å°: ${this.formatFileSize(file.size)}`;
                this.fileInfoCard.style.display = 'block';
            }

            handleReportTypeSelect(e) {
                // ç§»é™¤ä¹‹å‰çš„é€‰ä¸­çŠ¶æ€
                document.querySelectorAll('.report-type-card').forEach(card => {
                    card.classList.remove('selected');
                });
                
                // æ·»åŠ é€‰ä¸­çŠ¶æ€
                e.currentTarget.classList.add('selected');
                this.selectedReportType = e.currentTarget.dataset.type;
                this.updateUploadButton();
            }

            updateUploadButton() {
                const canUpload = this.selectedFile && this.selectedReportType;
                this.uploadBtn.disabled = !canUpload;
            }

            async handleUpload() {
                if (!this.selectedFile || !this.selectedReportType) {
                    this.showStatus('è¯·é€‰æ‹©æ–‡ä»¶å’ŒæŠ¥å‘Šç±»å‹', 'error');
                    return;
                }

                this.uploadStartTime = Date.now();
                this.lastUploadedBytes = 0;

                // æ„å»º FormData
                const formData = new FormData();
                formData.append('report_type', this.selectedReportType);
                formData.append('file', this.selectedFile);

                // æ˜¾ç¤ºè¿›åº¦åŒºåŸŸ
                this.uploadProgress.style.display = 'block';
                this.uploadBtn.disabled = true;
                this.uploadBtn.innerHTML = '<span class="loading-spinner"></span> ä¸Šä¼ ä¸­...';

                try {
                    const xhr = new XMLHttpRequest();
                    
                    // è¿›åº¦äº‹ä»¶ç›‘å¬
                    xhr.upload.addEventListener('progress', this.handleUploadProgress.bind(this));
                    
                    // å®Œæˆäº‹ä»¶
                    xhr.addEventListener('load', this.handleUploadComplete.bind(this));
                    
                    // é”™è¯¯äº‹ä»¶
                    xhr.addEventListener('error', this.handleUploadError.bind(this));
                    
                    // å–æ¶ˆäº‹ä»¶
                    xhr.addEventListener('abort', this.handleUploadAbort.bind(this));

                    // å¼€å§‹ä¸Šä¼ 
                    xhr.open('POST', '/health_reports/upload_with_progress');
                    xhr.setRequestHeader('X-CSRF-Token', document.querySelector('meta[name="csrf-token"]').content);
                    xhr.setRequestHeader('Accept', 'application/json');
                    
                    xhr.send(formData);

                } catch (error) {
                    this.handleUploadError(error);
                }
            }

            handleUploadProgress(e) {
                if (e.lengthComputable) {
                    const percentComplete = (e.loaded / e.total) * 100;
                    const uploadedBytes = e.loaded;
                    const totalBytes = e.total;
                    
                    // æ›´æ–°è¿›åº¦æ¡
                    this.progressBar.style.width = percentComplete + '%';
                    this.progressPercentage.textContent = Math.round(percentComplete) + '%';
                    
                    // æ›´æ–°å·²ä¸Šä¼ å¤§å°
                    this.uploadedSize.textContent = `å·²ä¸Šä¼ : ${this.formatFileSize(uploadedBytes)} / ${this.formatFileSize(totalBytes)}`;
                    
                    // è®¡ç®—ä¸Šä¼ é€Ÿåº¦
                    const currentTime = Date.now();
                    const timeElapsed = (currentTime - this.uploadStartTime) / 1000; // ç§’
                    const speed = uploadedBytes / timeElapsed; // bytes per second
                    this.uploadSpeed.textContent = `é€Ÿåº¦: ${this.formatFileSize(speed)}/s`;
                    
                    // è®¡ç®—å‰©ä½™æ—¶é—´
                    if (speed > 0) {
                        const remainingBytes = totalBytes - uploadedBytes;
                        const remainingTime = remainingBytes / speed; // ç§’
                        this.remainingTime.textContent = `å‰©ä½™æ—¶é—´: ${this.formatTime(remainingTime)}`;
                    }
                }
            }

            handleUploadComplete(e) {
                const xhr = e.target;
                
                this.uploadBtn.disabled = false;
                this.uploadBtn.innerHTML = '<span>ğŸ“¤</span> å¼€å§‹ä¸Šä¼ ';
                
                if (xhr.status === 200) {
                    try {
                        const response = JSON.parse(xhr.responseText);
                        
                        if (response.success) {
                            this.showStatus('æŠ¥å‘Šä¸Šä¼ æˆåŠŸï¼', 'success');
                            
                            // 2ç§’åè·³è½¬åˆ°æŠ¥å‘Šåˆ—è¡¨é¡µ
                            setTimeout(() => {
                                window.location.href = '/health_reports';
                            }, 2000);
                        } else {
                            this.showStatus(response.error || 'ä¸Šä¼ å¤±è´¥', 'error');
                        }
                    } catch (error) {
                        this.showStatus('è§£æå“åº”æ•°æ®å¤±è´¥', 'error');
                    }
                } else {
                    this.showStatus(`ä¸Šä¼ å¤±è´¥ (HTTP ${xhr.status})`, 'error');
                }
            }

            handleUploadError(error) {
                console.error('ä¸Šä¼ é”™è¯¯:', error);
                this.uploadBtn.disabled = false;
                this.uploadBtn.innerHTML = '<span>ğŸ“¤</span> å¼€å§‹ä¸Šä¼ ';
                this.showStatus('ä¸Šä¼ å¤±è´¥ï¼Œè¯·æ£€æŸ¥ç½‘ç»œè¿æ¥åé‡è¯•', 'error');
            }

            handleUploadAbort() {
                this.uploadBtn.disabled = false;
                this.uploadBtn.innerHTML = '<span>ğŸ“¤</span> å¼€å§‹ä¸Šä¼ ';
                this.showStatus('ä¸Šä¼ å·²å–æ¶ˆ', 'error');
            }

            showStatus(message, type) {
                this.statusMessage.textContent = message;
                this.statusMessage.className = `status-message status-${type}`;
                this.statusMessage.style.display = 'block';
                
                // 3ç§’åè‡ªåŠ¨éšè—é”™è¯¯æ¶ˆæ¯
                if (type === 'error') {
                    setTimeout(() => {
                        this.statusMessage.style.display = 'none';
                    }, 3000);
                }
            }

            formatFileSize(bytes) {
                if (bytes === 0) return '0 B';
                const k = 1024;
                const sizes = ['B', 'KB', 'MB', 'GB'];
                const i = Math.floor(Math.log(bytes) / Math.log(k));
                return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];
            }

            formatTime(seconds) {
                if (seconds < 60) return Math.round(seconds) + ' ç§’';
                const minutes = Math.floor(seconds / 60);
                const remainingSeconds = Math.round(seconds % 60);
                return minutes + ' åˆ† ' + remainingSeconds + ' ç§’';
            }
        }

        // åˆå§‹åŒ–ä¸Šä¼ å™¨
        document.addEventListener('DOMContentLoaded', () => {
            new ModernUploader();
        });
    </script>
